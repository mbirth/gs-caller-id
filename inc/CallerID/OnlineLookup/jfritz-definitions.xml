    <website name="dasoertliche.de" url="http://dasoertliche.de/?form_name=search_inv&amp;ph=$NUMBER&amp;page=5&amp;context=4&amp;action=43" prefix="0">
      <entry>
        <zipcode>pc: "([^"]*)",</zipcode>
        <city>ci: "([^"]*)",</city>
        <name>na: "([^"]*)",</name>
        <street>st: "([^"]*)",</street>
